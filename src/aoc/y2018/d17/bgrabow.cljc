(ns aoc.y2018.d17.bgrabow
  (:refer-clojure :exclude [read-string format])
  (:require
    [aoc.utils :as u :refer [deftest read-string format]]
    ;[aoc.y2018.d17.data :refer [input answer-1 answer-2]]
    [clojure.test :as t :refer [is testing]]
    [clojure.string :as str]))

(def axis-keyword {"x" :x "y" :y})

(defn parse-dimension [s]
  (let [[axis start-str end-str] (re-seq #"[x|y|\d]+" s)
        start (u/parse-int start-str)
        end (when end-str (u/parse-int end-str))]
    {(axis-keyword axis)
     (range start
            (if end
              (inc end)
              (inc start)))}))

(defn parse-coordinate-range [ss]
  (apply merge (map parse-dimension ss)))

(defn coordinate-pairs [{:keys [x y]}]
  (for [x' x y' y]
    [x' y']))

(defn parse-clay [input]
  (->> (str/split-lines input)
       (map #(str/split % #","))
       (map parse-coordinate-range)
       (mapcat coordinate-pairs)))

(defn below [p]
  (update p 1 inc))

(defn right [p]
  (update p 0 inc))

(defn left [p]
  (update p 0 dec))

(defn blocked? [cells point]
  (#{:clay :still-water} (cells point)))

(defn flow-right [cells outlet & uncertain-water]
  (cond (blocked? cells outlet)
        {:new-outlets     []
         :uncertain-water uncertain-water}
        (not (blocked? cells (below outlet)))
        {:new-outlets     [outlet (below outlet)]
         :uncertain-water (conj uncertain-water outlet)}
        :else
        (recur cells (right outlet) (conj uncertain-water outlet))))

(defn flow-left [cells outlet & uncertain-water]
  (cond (blocked? cells outlet)
        {:new-outlets     []
         :uncertain-water uncertain-water}
        (not (blocked? cells (below outlet)))
        {:new-outlets     [outlet (below outlet)]
         :uncertain-water (conj uncertain-water outlet)}
        :else
        (recur cells (left outlet) (conj uncertain-water outlet))))

(defn flow-sideways [{:keys [cells] :as m} outlet]
  (let [right (flow-right cells (right outlet))
        left (flow-left cells (left outlet))
        uncertain-water (-> [outlet]
                            (into (:uncertain-water right))
                            (into (:uncertain-water left)))
        new-outlets (into (:new-outlets right) (:new-outlets left))]
    (if (seq new-outlets)
      (-> m
          (update :consumed-outlets conj outlet)
          (update :new-outlets into new-outlets)
          (update :cells merge (zipmap uncertain-water (repeat :flowing-water))))
      (-> m
          (update :consumed-outlets conj outlet)
          (update :revisit-upwards conj outlet)
          (update :cells merge (zipmap uncertain-water (repeat :still-water)))))))

(defn flow-downwards [m outlet]
  (-> m
      (assoc-in [:cells outlet] :flowing-water)
      (update :new-outlets conj (below outlet))))

(defn flow-in-row [{:keys [cells] :as m} outlet]
  (cond (blocked? cells (below outlet))
        (flow-sideways m outlet)
        (nil? (cells (below outlet)))
        (flow-downwards m outlet)
        :else m))

(def print-cell
  (fnil {:clay          \#
         :still-water   \~
         :flowing-water \|
         :empty         \.}
        :empty))

(defn print-cells [cells]
  (let [x-min (apply min (map first (keys cells)))
        x-max (apply max (map first (keys cells)))
        y-min (apply min (map second (keys cells)))
        y-max (apply max (map second (keys cells)))]
    (map #(apply str %) (for [y (range y-min (inc y-max))]
                          (for [x (range x-min (inc x-max))]
                            (print-cell (cells [x y])))))))

(defn remove-consumed-outlets [consumed all]
  (reduce (fn [all [_ y :as p]]
            (update all y disj p))
          all consumed))

(defn add-new-outlets [new all]
  (reduce (fn [all [_ y :as p]]
            (update all y conj p))
          all new))

(defn simulate [clay spring]
  (let [y-min (apply min (map second clay))
        y-max (apply max (map second clay))
        initial-outlets (zipmap (range y-min (inc y-max)) (repeat #{}))]
    (loop [outlets (update initial-outlets (second spring) conj spring)
           old-cells (zipmap clay (repeat :clay))
           y (second spring)]
      (if (<= y y-max)
        (let [{:keys [cells new-outlets consumed-outlets] :as m}
              (reduce flow-in-row
                      {:cells       old-cells
                       :new-outlets []}
                      (outlets y))]
          (recur (->> outlets
                      (add-new-outlets new-outlets)
                      (remove-consumed-outlets consumed-outlets))
                 cells
                 (if (:revisit-upwards m) (dec y) (inc y))))
        old-cells))))

(defn solve-1 []
  (let [clay (parse-clay my-input)
        y-min (apply min (map second clay))
        y-max (apply max (map second clay))]
    (->>
      (simulate clay [500 0])
      (filter (fn [[[_ y] contents]]
                (and (#{:flowing-water :still-water} contents)
                     (<= y-min y y-max))))
      count)))

(defn solve-2 []
  (let [clay (parse-clay my-input)
        y-min (apply min (map second clay))
        y-max (apply max (map second clay))]
    (->>
      (simulate clay [500 0])
      (filter (fn [[[_ y] contents]]
                (and (#{:still-water} contents)
                     (<= y-min y y-max))))
      count)))

