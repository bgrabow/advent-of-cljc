(defn but-last [v]
  (split-at (dec (count v)) v))

(defn purge-anti-pairs-recursive
  ([coll]
   (purge-anti-pairs-recursive (take 0 coll)
                               (take 2 coll)
                               (drop 2 coll)))
  ([left mid right]
   (if (< (count mid) 2)
     (concat (reverse left) mid right)
     (let [found-pair (anti-pair? mid)]
       (cond
         ; Forget mid, create new mid from each side
         (and found-pair (seq left)) (recur (drop 1 left)
                                            (concat (take 1 left) (take 1 right))
                                            (drop 1 right))
         ; Forget mid, create new mid from only right side
         found-pair (recur left
                           (take 2 right)
                           (drop 2 right))
         ; Keep mid, move new mid one space right
         :else (recur (concat (take 1 mid) left)
                      (concat (drop 1 mid) (take 1 right))
                      (drop 1 right)))))))

; Divide and conquer is slower than single-threaded!
#_(defn divide-and-conquer [coll]
    (let [parts (partition-all (quot (count coll) 2) coll)]
      (purge-anti-pairs
        (apply concat (pmap purge-anti-pairs parts)))))

(defn purge-anti-pairs
  ([coll]
   (purge-anti-pairs (take 1 coll)
                     (drop 1 coll)))
  ([left right]
   (if (seq right)
     (if (anti-pair? (first left) (first right))
       (recur (rest left) (rest right))
       (recur (cons (first right) left)
              (rest right)))
     (reverse left))))

#_(defn purge-anti-pairs-iterative [s]
    (loop [coll (seq s)]
      (let [purged (seq (odd-removal (even-removal coll)))]
        (if (= coll purged)
          purged
          (recur purged)))))

(defn char-sets []
  (map (fn [l u] #{l u})
    (map char (range (int \a) (inc (int \z))))
    (map char (range (int \A) (inc (int \Z))))))

(defn solve-1 []
  (count (purge-anti-pairs input)))

(defn solve-2 []
  (let [partially-purged (purge-anti-pairs input)]
    (->> (pmap (fn [cs]
                 (->> partially-purged
                      (remove cs)
                      purge-anti-pairs
                      count))
              (char-sets))
         (apply min))))

(deftest part-1
